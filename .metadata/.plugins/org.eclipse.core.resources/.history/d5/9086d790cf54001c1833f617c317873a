import java.util.Scanner;

public class CharPattern3 {
/*
 *  A
 	BB
 	CCC
 * */
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int i =1;
		
		while(i<=n) {
			int j=1;
			int a = (int)('A' +1);
			char ch = (char)('A'+1);
			while(j<=i) {
				System.out.print(a);
//				ch = (char)(ch + 1);
				j++;
			}
			System.out.println();
			i++;
		}
		
		sc.close();
	}

}
