package com.pratmodi.one;

import java.util.Scanner;

public class Solution9 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		System.out.println();
		for(int i=0;i<n;i++) {
			int d = sc.nextInt();
			for(int j=0;j<d;j++) {
				int dx = sc.nextInt();
			}
		}
		int q = sc.nextInt();
		System.out.println();
		for(int i=0;i<q;i++) {
			int q1 = sc.nextInt();
			int q2 = sc.nextInt();
		}
	}

}
