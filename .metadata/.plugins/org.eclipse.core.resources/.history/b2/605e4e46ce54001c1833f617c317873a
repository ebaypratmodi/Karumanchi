import java.util.Scanner;

public class CharPattern3 {
/*
 *  A
 	BB
 	CCC
 * */
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int i =1;
		while(i<=n) {
			int j=1;
			char ch = (char)('A'+j);
			while(j<=n) {
				System.out.print(ch);
			}
		}
		
		sc.close();
	}

}
