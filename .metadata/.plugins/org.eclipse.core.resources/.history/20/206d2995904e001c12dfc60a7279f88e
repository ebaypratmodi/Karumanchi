
public class Circular {

	public static void main(String[] args) {

		String target = "7531";

		String[] blocked = { "1543", "7434", "7300", "7321", "2427" };
		String sample ="0000";
		int rotationCounter;

		for (int i = 0; i < blocked.length; i++) {
			for (int j = i + 1; j <= blocked.length; j++) {
				sample.charAt(i);
			}
		}

	}

}
