import java.util.Scanner;

public class Solution4 {

	/*
	 * 1 21 321 4321
	 */

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();

		for (int i = 1; i <= n; i++) {
			for (int j = 1; j <= i; j++) {
				for (int z = 0; z < j; z++) {
					System.out.print(i);
				}
			}
			System.out.println();
		}
		sc.close();

	}

}
