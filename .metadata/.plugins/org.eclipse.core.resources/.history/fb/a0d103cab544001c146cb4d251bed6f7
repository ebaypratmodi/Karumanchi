package com.pratmodi.one;

import java.util.Scanner;

public class Solution2 {

	 public static void main(String[] args) {
         Scanner sc=new Scanner(System.in);
         System.out.println("================================");
         for(int i=0;i<3;i++)
         {
             String s1=sc.next();
             int x=sc.nextInt();
             sc.nextLine();
             System.out.printf("'%-15s' %n", s1);
             if(x>99 && x<1000)
             System.out.printf(x);
         }
         System.out.println("================================");

 }
	
}
