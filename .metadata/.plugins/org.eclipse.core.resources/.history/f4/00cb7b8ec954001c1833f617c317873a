import java.util.Scanner;

/*
 *	1
	21
	321
	4321 
 * 
 * */

public class Solution4 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();

		for (int i = n; i >= n; i--) {
			for (int j = n; j >= i; j--) {
				System.out.print(j);
			}
			System.out.println();
		}
		sc.close();
	}
}
