import java.util.Scanner;

public class CharPattern {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int i=1;
		int j=1;
		while(i<n) {
			while(j<=n) {
				System.out.print((char)('A'+j-1));
				j++;
			}
			System.out.println();
			i++;
		}
		
	}

}
